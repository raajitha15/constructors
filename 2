class Student 
{
	String name;//constructor used to initialize
	Student(String str)
	{
		name=str;
	}
	public static void main(String[] args) 
	{
		Student s1=new Student("Jagannth");
		System.out.println("Name of the student 1" + s1.name);
		Student s2=new Student("James");
		System.out.println("Name of the student 2" + s2.name);
		Student s3=new Student("Jhon");
		System.out.println("Name of the student 3" + s3.name);
	}
}
