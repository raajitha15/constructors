class Example1 
{
	Example1()
	{
		System.out.println("no parameter constructor");
	}
	Example1(int num)
	{
		System.out.println("int parameter constructor");
	}
	Example1(Object data)
	{
		System.out.println("object parameter constructor");
	}
	Example1(String str)
	{
		System.out.println("String parameter constructor");
	}
}

	class TestConstructorOverloading
	{
		public static void main(String[]args)
		{
			//Example1 obj=new Example1('A');
			//Example1 obj=new Example1(5L);
			//Example1 obj=new Example1("A");
			//Example1 obj=new Example1(null);
			//Example1 obj=new Example1(10.2);
			//Example1 obj=new Example1((Integer)null);
			//Example1 obj=new Example1((String)null);
			Example1 obj=new Example1((Double)null);

	}
	
}

